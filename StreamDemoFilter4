import java.util.*;

class StreamDemoFilter4 {
    public static void main(String arg[]) {

        List<String> names = Arrays.asList("Mahesh", "Aishwarya", "Pop", "Nitin");

        List<String> result = 
            names.stream()
            .filter(s -> s.equals(new StringBuilder(s).reverse().toString()))   
            .toList();

        System.out.println(result);
 
    }
}
